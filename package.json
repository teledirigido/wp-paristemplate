    "http-proxy-middleware": "^0.19.1",
    "laravel-mix": "^4.0.15",
    "laravel-mix-purgecss": "^4.1.0",
    "resolve-url-loader": "2.3.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "font-awesome-stylus": "^4.7.1",
    "smoothscroll-polyfill": "^0.4.3",
    "stickyfilljs": "^2.1.0",
    "swiper": "^4.5.0"
